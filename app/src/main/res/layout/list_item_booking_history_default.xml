<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/cardView"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/card_layout_margin"
    android:minHeight="@dimen/card_history_min_height"
    app:cardBackgroundColor="@color/colorCardDefault"
    app:cardCornerRadius="@dimen/card_corner_radius"
    app:cardElevation="@dimen/card_default_elevation">

    <include
        layout="@layout/include_booking_history_date_seats"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</android.support.v7.widget.CardView>



